  "devDependencies": {
    "electron": "^35.7.5",
    "electron-builder": "^26.7.0",
    "@electron/rebuild": "^4.0.1",
    "typescript": "^5.0.4"
  },
  "keywords": [
    "capacitor",
    "electron"
  ]
